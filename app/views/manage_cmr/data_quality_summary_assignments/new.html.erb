<div class="grid-row row-content">
  <section>
    <h2>New <%= current_user.provider_id %> Data Quality Summary Assignments</h2>

    <%= form_tag(data_quality_summary_assignments_path, method: :post, class: "data-quality-summary-form", id: "data-quality-summary-assignments-form") do %>
      <%= render partial: "form" %>

      <fieldset>
        <%= submit_tag('Submit', class: 'eui-btn--blue') %>
        <%= link_to 'Cancel', :back, class: 'eui-btn' %>
      </fieldset>
    <% end %>
  </section>
</div>
